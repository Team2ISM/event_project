<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
 xmlns="urn:nhibernate-mapping-2.2"
 assembly="Events.NHibernateDataProvider"
 namespace="Events.Business.Models"
 auto-import="true">
  <class name="EventModel, Events.Business" table="Event">
    <id name="Id" access="property" type="string">
      <generator class="assigned"></generator>
    </id>
    <property name="Title" access="property" type="string"></property>
    <property name="Description" access="property" type="string"></property>
    <property name="LongDescription" access="property" type="string"></property>
    <property name="From" access="property" column="FromDate" type="datetime"></property>
    <property name="To" access="property" column="ToDate" type="datetime"></property>
    <property name="Location" access="property" type="string"></property>
  </class>
</hibernate-mapping>